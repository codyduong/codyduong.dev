{
  "name": "codyduongweb",
  "version": "0.1.0",
  "private": true,
  "main": "build/functions.js",
  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "concurrently \"yarn start:tsc\" \"razzle start\"",
    "start:firebase": "concurrently \"yarn start\" \"firebase emulators:start\"",
    "build:lite": "tsc -b && razzle build --noninteractive",
    "export:no_build": "echo deprecated; exit 1 && razzle export && scripts/cp404.sh",
    "export": "echo deprecated; exit 1 && yarn build:lite && yarn export:no_build",
    "build": "cross-env-shell NODE_ENV=production scripts/rm.sh && yarn build:lite",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "serve": "yarn start:prod",
    "test": "razzle test --env=jsdom",
    "eject": "exit 1 && react-scripts eject",
    "storybook": "cross-env TS_NODE_PROJECT=tsconfig.storybook.json start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "ci": "yarn install --frozen-lockfile",
    "lint": "yarn eslint",
    "lint:changed": "./scripts/lint.sh",
    "postinstall": "./scripts/post-install.sh",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/overpass": "^4.5.8",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@react-three/a11y": "^3.0.0",
    "@react-three/cannon": "^6.5.0",
    "@react-three/drei": "^9.43.3",
    "@react-three/fiber": "^8.9.1",
    "@theatre/core": "0.5",
    "@theatre/r3f": "0.5",
    "@theatre/studio": "0.5",
    "@types/jest": "^28.1.2",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.141.0",
    "classnames": "^2.3.1",
    "color": "^4.2.3",
    "concurrently": "^7.6.0",
    "cross-fetch": "^3.1.5",
    "firebase-admin": "^11.3.0",
    "firebase-functions": "^4.1.0",
    "geojson-vt": "^3.2.1",
    "global": "^4.4.0",
    "graphql": "^16.6.0",
    "node-sass": "^7.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6",
    "react-scripts": "5.0.1",
    "react-spring": "^9.4.5",
    "spinkit": "^2.0.1",
    "styled-components": "^5.3.5",
    "three": "^0.146.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@craco/craco": "^7.0.0-alpha.9",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/color": "^3.0.3",
    "@types/loadable__server": "^5.12.6",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-razzle": "^4.2.18",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-codyduong": "https://github.com/codyduong/eslint-config-codyduong.git#semver:v1.0.2",
    "eslint-mdx": "^1.17.0",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "firebase-tools": "^11.16.1",
    "html-webpack-plugin": "5.2.0",
    "mini-css-extract-plugin": "2.7.0",
    "postcss": "8.4.19",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "razzle": "^4.2.18",
    "razzle-dev-utils": "^4.2.18",
    "react-app-alias": "^2.2.0",
    "serve": "^14.0.1",
    "source-map-support": "^0.5.21",
    "storybook-addon-react-router-v6": "^0.1.10",
    "storybook-preset-craco": "https://github.com/artisanofcode/storybook-preset-craco.git",
    "tsconfig-paths": "^4.0.0",
    "webpack": "5",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "3.11.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "quotes": true,
    "endOfLine": "auto"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/packages"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "packages/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "resolutions": {
    "autoprefixer": "^10.4.13"
  }
}
