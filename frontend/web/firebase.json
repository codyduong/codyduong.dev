{
  "hosting": {
    "public": "./dist/client",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/index.html"
    ],
    "rewrites": [
      {
        "glob": "**",
        "run": {
          "serviceId": "web",
          "region": "us-central1"
        }
      }
    ]
  }
}