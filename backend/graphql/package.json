{
  "name": "codyduongweb-graphql",
  "version": "0.1.0",
  "private": "true",
  "main": "dist/functions.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
    "start": "node dist/server",
    "clean": "./scripts/clean.sh",
    "build": "yarn -s run clean && yarn -s run generate && tsc",
    "generate": "yarn -s run generate:prisma && yarn -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "quotes": true,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@apollo/server": "^4.3.1",
    "@prisma/client": "4.9.0",
    "apollo-server-express": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.3.0",
    "firebase-functions": "^4.1.0",
    "glob": "^8.0.3",
    "graphql": "16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-scalars": "1.20.1",
    "graphql-shield": "^7.6.5",
    "jsonwebtoken": "^9.0.0",
    "nexus": "1.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/glob": "^8.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.11.18",
    "cross-env": "^7.0.3",
    "eslint-config-codyduong": "https://github.com/codyduong/eslint-config-codyduong.git#semver:v1.0.2",
    "nodemon": "^2.0.20",
    "prisma": "4.9.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
